"\n"
"  <link xref=\"mal_info\">mal_info</link> ?,\n"
"  <link xref=\"mal_block_title\">mal_block_title</link>,\n"
"  <link xref=\"mal_block\">mal_block</link> *,\n"
"  <link xref=\"mal_section\">mal_section</link> *\n"
"}\n"
